<h2>商品新規登録</h2>

<table class="table table-hover table-inverse">
  <tbody>
    <tr>
      <td>
        商品画像
        <%= f.file_field :image, accept: "image/*" %>
      </td>
      <td>
        商品名
        <%= f.text_field :name %>
      </td>
      <td>
        商品説明
        <%= f.text_field :introduction %>
      </td>
      <td>
        ジャンル

      </td>
      <td>
        税抜価格
        <%= f.text_field :price %>
      </td>
    </tr>

  </tbody>

</table>
